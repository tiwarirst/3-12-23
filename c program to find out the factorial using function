#include <stdio.h>
int fac(int a)
{
    int j=1;
  if (a==0)
    return 1;
  else
   for(int i=1;i<=a;i++)
   {j=j*i;}
    return j;
}
int main()
{
    int x;
    printf("enter the  number :");
    scanf("%d",&x);
    if(x<0)
    printf("no factorial of negative no");
    else
    printf("factorial of number is:%d",fac(x));
    return 0;
}
