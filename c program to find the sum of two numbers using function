#include <stdio.h>
int fan(int a,int b)
{
    int c;
    c=a+b;
    printf ("the sum is :%d",c);
}
int main()
{
    int j;
    int x,y;
    printf("enter the first number :");
    scanf("%d",&x);
    printf("enter the second number");
    scanf("%d",&y);
    j=fan(x,y);
    printf("Hello World");

    return 0;
}
